	&ibm
	!==============================================================!
	! Buoyancy Module
	Buoyancy          	= .false.
	buoyancyFilename  	= "buoyancy_example"
	!==============================================================!
	! Seascape Module
	polygon		        = .false.
	polyFilename 	    	= "poly_example.xyz"
	settlementStart	  	= 2		!in days
	!==============================================================!
	! Probability Matrix of Vertical Migration
	ibio 			= .true.
	ibioFilename	    	= "vertmig.txt"
	ibioTimeStep	    	= 2400		!in seconds
	!==============================================================!
	! Mortality Rate
	mort			= .false.
	halflife		= 90000		!in seconds
	!==============================================================!
	! Different Particle Attributes Module
	diffpart		= .false.
	diffpartFilename  	= ""
	!==============================================================!
	! Combined Buoyancy and Vertical Migration 
	massSpawning		= .false.
	larvaStart		= 7		!in days
	!==============================================================!
	! Selective Tidal Stream Transport 
	tidalmovement		= .false.
	tstStart		= 14		!in days
	!==============================================================!
	! Adding strata for 3D polygons
	strata			= .false.
	strataFilename		= "strata_example"
	!==============================================================!
	! Output temperature/salinity in netcdf file
	outputtemp		= .false.
	outputsaln		= .false.
	!==============================================================!
	$end
